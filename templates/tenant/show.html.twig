{% extends 'base.html.twig' %}

{% block title %}Tenant{% endblock %}
{% block page_title %}{{ 'tenat.title_detail'|trans }}{% endblock %}
{% block page_content %}
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0">{{ 'label.depense_new'|trans }}</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">{{ 'label.depense_list'|trans }}</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<div class="card">
    <div class="card-body">

    <table class="table">
        <tbody>
            <tr class="col-md-6">
                <th>Id</th>
                <td>{{ tenant.id }}</td>
            </tr>
            <tr class="col-md-6">
                <th>{{ 'tenant.table.name'|trans }}</th>
                <td>{{ tenant.name }}</td>
            </tr>
            <tr class="col-md-6">
                <th>Situation</th>
                <td>{{ tenant.situation }}</td>
            </tr>
            <tr class="col-md-6">
                <th>{{ 'tenant.table.number_child'|trans }}</th>
                <td>{{ tenant.numberChild }}</td>
            </tr>
            <tr class="col-md-6">
                <th>{{ 'tenant.table.profession'|trans }}</th>
                <td>{{ tenant.profession }}</td>
            </tr>
            <tr class="col-md-6">
                <th>{{ 'tenant.table.cni'|trans }}</th>
                <td>{{ tenant.cni }}</td>
            </tr>
            <tr class="col-md-6">
                <th>{{ 'tenant.table.nationality'|trans }}</th>
                <td>{{ tenant.nationality }}</td>
            </tr>
            <tr class="col-md-6">
                <th>{{ 'tenant.table.phone'|trans }}</th>
                <td>{{ tenant.phone }}</td>
            </tr>
            <tr class="col-md-6">
                <th>{{ 'tenant.table.email'|trans }}</th>
                <td>{{ tenant.email }}</td>
            </tr>
            <tr class="col-md-6">
                <th>Photo</th>
                <td>{{ tenant.photo }}</td>
            </tr>
            <tr class="col-md-6">
                <th>CreatedAt</th>
                <td>{{ tenant.createdAt ? tenant.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr class="col-md-6">
                <th>UpdatedAt</th>
                <td>{{ tenant.updatedAt ? tenant.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    </div></div>
<div class="card">
    <div class="card-body">
<div class="table-responsive">
    <table class="table">
        <thead>
        <tr>
            <th>Id</th>
            <th>{{ 'rental.table.type'|trans }}</th>
            <th>{{ 'rental.table.locals'|trans }}</th>
            <th>
                Status
            </th>
        </tr>
        </thead>
        <tbody>
        {% for contrat in tenant.rentalContracts %}
        <tr>
            <td>{{ contrat.id }}</td>
            <td>{{ contrat.typeRental }}</td>
            <td>{{ contrat.locals |join('|') }}</td>
            <td>{{ contrat.status}}
                {% if contrat.status %}
                    <span class="btn btn-xs btn-success"><i
                                class="fa fa-check-circle"></i></span>
                {% else %}
                    <span class="btn btn-xs btn-danger"> <i
                                class="fa fa-times-circle"></i></span>
                {% endif %}</td>
        </tr>
        {% else %}
            <tr>
                <td colspan="13">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>
    </div></div>
{% endblock %}
